{
    "env": {
    "browser": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "no-console": "off"
    },
    "parserOptions": {
    "ecmaVersion": 2017
    },
    "rules": {
    "indent": [
    "error",
    4
    ],
    "linebreak-style": [
    "error",
    "windows"
    ],
    "quotes": [
    "error",
    "single"
    ],
    "semi": [
    "error",
    "always"
    ]
    }
    }